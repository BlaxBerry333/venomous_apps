{
  "name": "admin_client",
  "private": true,
  "version": "1.0.0",
  "license": "UNLICENSED",
  "author": "blaxberry333",
  "description": "blaxberry333's venomous apps' admin client",
  "type": "module",
  "scripts": {
    "start:dev": "vite --host --mode development",
    "start:prod": "vite --host --mode production",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "commit": "cd .. && npm run commit",
    "check-packages": "npx depcheck",
    "check-eslint": "eslint . --cache --cache-location=./.cache/.eslintcache",
    "check-stylelint": "stylelint \"**/*.{css,scss}\"",
    "check-prettier": "prettier . --check",
    "check-all": "yarn check-eslint && yarn check-stylelint && yarn check-prettier",
    "eslint": "prettier --write .",
    "stylelint": "stylelint \"**/*.{css,scss}\" --fix --cache --cache-location=./.cache/.stylelintcache",
    "prettier": "prettier . --write --cache --cache-location=./.cache/.prettiercache",
    "format-all": "npm run eslint && npm run prettier && npm run stylelint"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hookform/resolvers": "^3.9.1",
    "@mui/lab": "^6.0.0-beta.14",
    "@mui/material": "^6.1.5",
    "@tanstack/react-query": "^5.59.19",
    "@tanstack/react-query-devtools": "^5.59.19",
    "@xyflow/react": "^12.3.4",
    "lodash-es": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.53.1",
    "react-router-dom": "^6.27.0",
    "zod": "^3.23.8",
    "zundo": "^2.2.0",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@faker-js/faker": "^9.2.0",
    "@hookform/devtools": "^4.3.1",
    "@iconify/react": "^5.0.2",
    "@tanstack/eslint-plugin-query": "^5.59.7",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.80.3",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  }
}
