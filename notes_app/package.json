{
  "name": "venomous_notes_app",
  "private": true,
  "version": "1.0.0",
  "license": "UNLICENSED",
  "author": "blaxberry333",
  "description": "blaxberry333's venomous apps' notes app",
  "type": "module",
  "scripts": {
    "start:dev": "nuxt dev --dotenv .env.development",
    "start:prod": "node .output/server/index.mjs",
    "build:dev": "nuxt build --dotenv .env.development",
    "build:prod": "nuxt build --dotenv .env.production",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "commit": "cd .. && npm run commit",
    "check-packages": "npx depcheck",
    "check-eslint": "eslint . --cache --cache-location=./.eslintcache",
    "check-prettier": "prettier . --check",
    "check-all": "npm run check-eslint && npm run check-prettier",
    "eslint": "prettier --write .",
    "prettier": "eslint . --fix --cache --cache-location=./.prettiercache",
    "format-all": "npm run eslint && npm run prettier"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "mongoose": "^8.7.2",
    "nuxt": "^3.13.2",
    "vue": "^3.5.10",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "eslint": "^9.13.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.29.1",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "typescript-eslint": "^8.11.0",
    "vite-plugin-vuetify": "^2.0.4",
    "vuetify": "^3.7.3"
  }
}
